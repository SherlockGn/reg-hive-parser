{
    "name": "reg-hive-parser",
    "version": "1.0.1",
    "description": "An entirely JavaScript-based tool for parsing Windows Registry hive files, including the NTUSER.dat file, enhancing the security of the analysis process.",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "type": "module",
    "exports": {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SherlockGn/reg-hive-parser.git"
    },
    "scripts": {
        "build": "rollup -c",
        "test": "node test/test.cjs && node test/test.mjs"
    },
    "keywords": [
        "windows",
        "registry",
        "hive",
        "parser",
        "security"
    ],
    "author": "Neko Gong",
    "license": "MIT",
    "devDependencies": {
        "rollup": "^4.14.1"
    }
}
